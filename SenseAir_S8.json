{
	"sensors": [

		{
			"name": "SenseAir S8 Custom",
			"command": "FE 04 00 00 00 05 24 06",
			"send_cmd_period": 4,
			"port": {
				"baudRate": 9600,
				"dataBits": 8,
				"stopBits": 1,
				"parity": "none"
			},
			"frame": {
				"length": 15,
				"startByte": ["0xFE", "0x04"],
				"endByte": "none"
			},
			"data": {
				"CO2": {
					"value": "(data[9] << 8) + data[10]",
					"unit": "ppm"
				},
				"Temperature": {
					"value": "((data[11] << 8) + data[12])/100",
					"unit": "Â°C"
				}
			},
			"checksum": {
				"eval": "data.slice(0,13).reduce((c,b)=>(c^=b,Array.from({length:8},()=>c=(c&1)?(c>>1)^0xA001:c>>1),c),0xFFFF)",
				"compare": "(data[14]<<8)|data[13]"
			}
		}
	]	
}