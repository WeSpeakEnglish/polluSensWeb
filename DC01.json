{
	"sensors": [
     {
			"name": "DC01",
			"command": "none",
			"port": {
				"baudRate": 9600,
				"dataBits": 8,
				"stopBits": 1,
				"parity": "none"
			},
			"frame": {
				"length": 4,
				"startByte": "0xA5",
				"endByte": "none"
			},
			"data": {
				"PM2.5": {
					"value": "(data[1] << 7) + data[2]",
					"unit": "μg/m³"
				},
				"PM2.5 x0.4 calibrated": {
					"value": "(data[1] << 7) + data[2]",
					"unit": "μg/m³"
				}
			},
			"checksum": {
				"eval": "(data[0] + data[1] + data[2])&0x7F",
				"compare": "data[3]"
			}
		}
	]	
}