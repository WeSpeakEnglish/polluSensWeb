  "data": {
    "pm1": "data[1] + (data[2] << 8) + (data[3] << 16) + (data[4] << 24)",
    "pm25": "data[5] + (data[6] << 8) + (data[7] << 16) + (data[8] << 24)",
    "pm10": "data[9] + (data[10] << 8) + (data[11] << 16) + (data[12] << 24)"
  },
  "checksum": {
    "eval": "data.slice(1, 30).reduce((a, b) => a ^ b, 0)",
	"compare": "data[30]"
  },
